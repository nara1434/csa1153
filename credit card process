  // Methods
  //


  //
  // Accessor methods
  //

  /**
   * Set the value of item_purchased
   * @paramnewVar the new value of item_purchased
   */
private void setItem_purchased (void newVar) {
item_purchased = newVar;
  }

  /**
   * Get the value of item_purchased
   * @return the value of item_purchased
   */
private void getItem_purchased () {
returnitem_purchased;
  }

  /**
   * Set the value of credit_card
   * @paramnewVar the new value of credit_card
   */
private void setCredit_card (void newVar) {
credit_card = newVar;
  }

  /**
   * Get the value of credit_card
   * @return the value of credit_card
   */
private void getCredit_card () {
returncredit_card;
  }

  //
  // Other methods
