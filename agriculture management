/**
 * Class person
 */
public class person {

  //
  // Fields
  //

private void name;
private void address;
private void phone_no;
private void email_id;

  //
  // Constructors
  //
public person () { };

  //
  // Methods
  //


  //
  // Accessor methods
  //

  /**
   * Set the value of name
   * @paramnewVar the new value of name
   */
private void setName (void newVar) {
name = newVar;
  }

  /**
   * Get the value of name
   * @return the value of name
   */
private void getName () {
return name;
  }

  /**
   * Set the value of address
   * @paramnewVar the new value of address
   */
private void setAddress (void newVar) {
address = newVar;
  }

  /**
   * Get the value of address
   * @return the value of address
   */
private void getAddress () {
return address;
  }

  /**
   * Set the value of phone_no
   * @paramnewVar the new value of phone_no
   */
private void setPhone_no (void newVar) {
phone_no = newVar;
  }

  /**
   * Get the value of phone_no
   * @return the value of phone_no
   */
private void getPhone_no () {
returnphone_no;
  }

  /**
   * Set the value of email_id
   * @paramnewVar the new value of email_id
   */
private void setEmail_id (void newVar) {
email_id = newVar;
  }

  /**
   * Get the value of email_id
   * @return the value of email_id
   */
private void getEmail_id () {
returnemail_id;
  }

  //
  // Other methods
